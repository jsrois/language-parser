# THESE ARE GENERAL SETTINGS. DO NOT EDIT THIS FILE

LIBRARY_FILES       = $${PWD}/libfiles.pri
COPYFILES_SCRIPT    = $${PWD}/copyfiles.pri
SOURCE_PATH         = $${PWD}/lib
TEST_PATH           = $${PWD}/test
DEPENDENCIES_PATH   = $${PWD}/../dependencies
BUILD_PATH          = $${PWD}/../build

# With C++11 support
greaterThan(QT_MAJOR_VERSION, 4){
CONFIG += c++11
} else {
QMAKE_CXXFLAGS += -std=c++0x
}

# Tidy up on each clean
QMAKE_CLEAN += *.gcda *.gcno *.o
